  "devDependencies": {
    "eslint": "^4.2.0",
    "eslint-config-brightspace": "^0.3.1",
    "eslint-plugin-html": "^3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "polymer-cli": "^1.3.1",
    "sinon": "^5.0.7"
  },
  "dependencies": {
    "bower-art-resolver": "^2.0.9",
    "npm": "^6.0.0"
  },
  "lint-staged": {
    "*.html": [
      "eslint",
      "git add"
    ]
